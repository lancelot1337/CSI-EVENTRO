<h1>New Event</h1>
<%= form_for @event do |f| %>
    <p>
        <%= f.label :title %>
        <br>
        <%= f.text_field :title %>
        <br>
        <%= f.label :description %>
        <br>
        <%= f.text_area :description %>
        <br>
        <%= f.label :venue %>
        <br>
        <%= f.text_field :venue %>
        <br>
        <%= f.label :startsat %>
        <br>
        <%= f.date_field :startsat %>
        <br>
        <%= f.label :endsat %>
        <br>
        <%= f.date_field :endsat %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>